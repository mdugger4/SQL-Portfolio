INSERT INTO puppy_adoption VALUES (9, "maltipoo", 2, 175);
INSERT INTO puppy_adoption VALUES (10, "schnauzer", 5, 275);

SELECT * FROM puppy_adoption ORDER BY breed;

SELECT SUM(quantity) FROM puppy_adoption;


